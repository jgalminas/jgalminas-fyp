{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  // "exclude": [
  //   "src/preload/index.d.ts"
  // ],
  "compilerOptions": {
    "types": [
      "vite-plugin-svgr/client"
    ],
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "outDir": "out/preload",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
